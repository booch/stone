
Array := Class[T = Anything]({

    .superclass := List[T]  # This might eventually become an implementation of List, instead of a subclass.

    # Note: index is an Ordinal, so it starts at 1.
    @at(index :: Number.Integer.Ordinal) := { ... }

    @last := { ... }
    @take(n :: Number.Integer.Cardinal)
}
